  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run EjercicioIntegrador-EjercicioIntegrador",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/ejerciciointegrador-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/EjercicioIntegradorEjercicioIntegrador/index.html",
        "--ui5",
        "{\"version\":\"1.84.8\"}"
      ],
      "cwd": "${workspaceFolder}/EjercicioIntegrador",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/EjercicioIntegrador/EjercicioIntegrador\"}"
      },
      "envFile": "${workspaceFolder}/EjercicioIntegrador/.env2",
      "preLaunchTask": "Build EjercicioIntegrador"
    }
  ]
}